    script.
        $(document).ready(function() {
        var t = $('#restable').DataTable();
        //$("#btn1").click(function(){
        $.ajax({
        type: "GET",
        url: "/api/members/",
              }).done(function(data){
        //  console.log("Response is:"+data.id+"-"+data.name);
        $.each(data, function(item, obj) {
        console.log("Response is:"+obj.FC_CODE+"-"+obj.MEMBER_NAME);
        t.row.add(
        [ obj.FC_CODE,
        obj.MEMBER_NAME,
        obj.MEMBER_DOB,
        obj.BIRTH_COUNTRY,
        obj.RESIDENCY_COUNTRY,
        obj.PASSPORT_NUMBER,
        obj.EMAIL,
        obj.PHONENUMBER
        ]).draw(false);
        
        //});
        })
        })
        });
